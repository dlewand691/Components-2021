    mixin animated-text-carousel(BGImage, ID, expandValue, script, buttonText, buttonLink)
        .slide.text-center(style="width:1600px; height:500px; background: url("+BGImage+") no-repeat; background-size: cover; background-position: center center;")
            .flex-container.align-bottom.align-center.relative.lazyload.overflow-hidden(id=ID data-expand=expandValue data-script=script style="width:1600px; height:500px;")

                a.button.arrow.small.z4.small-mb3(href=buttonLink)!= buttonText

