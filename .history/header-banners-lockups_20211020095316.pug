extend __LAYOUT-TEMPLATE.pug
block vars
    - var pageTitle = "Header banners lockups"

block content

    h1!= pageTitle
    .grid-x.grid-padding-x.cell
        h3 CDB: Ai & Analytics

    .banner-header(data-interchange="[images/header-banners/logo-lockups/bg-header-ai-mobile.jpg, small],[images/header-banners/logo-lockups/bg-header-ai-tablet.jpg, medium],[images/header-banners/logo-lockups/bg-header-ai-desktop.jpg, large]")
        .grid-x.grid-padding-x.py2.medium-py4.large-py7
            .cell.small-12.medium-4.large-6
                img.banner-logoLockup.p1.medium-p2.large-p3(src="images/header-banners/logo-lockups/CDB-logo-lockup-ai-analytics.svg")
            .cell.small-12.medium-8.large-6.text-white
                h6.label.text-wrap.hide-for-small-only Rehumanizing AI Journey
                h3 CUSTOMER-CENTRIC AI SOLUTIONS AT SCALE

    hr#cdb-ai-analytics-video-background
    .grid-x.grid-padding-x.cell
        h3 CDB: Ai & Analytics, Video background

    .grid-x.grid-padding-x.expanded.align-middle.video-background.h500px(style="background-size: cover; background-position: center center;" data-interchange="[images/header-banners/video-background/bg-header-video-ai-analytics.jpg, small],[images/header-banners/video-background/bg-header-video-ai-analytics.jpg, medium],[../images/transparent.gif, large]")
        video.hide-for-small-only(playsinline autoplay loop muted poster="images/header-banners/video-background/bg-header-video-ai-analytics.jpg")
            source(src="images/header-banners/video-background/bg-header-video-ai-analytics.mp4" type="video/mp4")
            source(src="images/header-banners/video-background/bg-header-video-ai-analytics.webm" type="video/webm")
            source(src="images/header-banners/video-background/bg-header-video-ai-analytics.ogg" type="video/ogg")
        .grid-x.grid-padding-x.align-middle(style="min-height: 500px")
            .cell.small-12.medium-4.large-6
                img.banner-logoLockup.p1.medium-p2.large-p3(src="images/header-banners/logo-lockups/CDB-logo-lockup-ai-analytics.svg")
            .cell.small-12.medium-8.large-6.text-white
                h6.label.text-wrap.hide-for-small-only CX-Centric AI at Scale
                h3 Rehumanizing the AI Journey
                a.button.arrow(href="#") VIEW VIDEO

    hr
    .grid-x.grid-padding-x.cell#ai-banner-video
        h3 CDB: Ai & Analytics, video background/play button
    .banner-header.video-background(style="background: rgba(2,37,45,.6); background-size: cover; background-position: center center;" data-interchange="[images/header-banners/logo-lockups/bg-video-ai-analytics-mobile.jpg, small],[images/header-banners/logo-lockups/bg-video-ai-analytics-tablet.jpg, medium],[../images/transparent.gif, large]")
        video.show-for-large(playsinline autoplay loop muted poster="images/header-banners/logo-lockups/bg-video-ai-analytics-desktop.jpg")
            source(src="images/header-banners/logo-lockups/bg-video-ai-analytics.mp4" type="video/mp4")
            source(src="images/header-banners/logo-lockups/bg-video-ai-analytics.webm" type="video/webm")
            source(src="images/header-banners/logo-lockups/bg-video-ai-analytics.ogg" type="video/ogg")
            .grid-x.grid-padding-x.py2.medium-py4.large-py7
                .cell.small-12.medium-4.large-6
                    img.banner-logoLockup.p1.medium-p2.large-p3(src="images/header-banners/logo-lockups/CDB-logo-lockup-ai-analytics.svg")
                .cell.small-12.medium-8.large-6.text-white
                    h6.label.text-wrap.hide-for-small-only Insights that Inspire Customers
                    h3 ACCELERATE BUSINESS GROWTH WITH AI
                    a.button.white.arrow(data-open="video-modal-ai") play video
                .reveal#video-modal-ai(data-reveal data-animation-in="scale-in-up" data-animation-out="scale-out-down")
                    .responsive-embed.widescreen
                        iframe(width="560", height="315", src="https://www.youtube-nocookie.com/embed/xgtdBJuaY-Y", frameborder="0", allow="autoplay; encrypted-media", allowfullscreen="")
                    button.close-button(data-close aria-label="Close modal" type="button")
                        span(aria-hidden="true") &times;


